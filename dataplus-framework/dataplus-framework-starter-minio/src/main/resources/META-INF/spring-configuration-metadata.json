{
  "groups": [
    {
      "name": "minio",
      "type": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties"
    }
  ],
  "properties": [
    {
      "name": "minio.access-key",
      "type": "java.lang.String",
      "description": "账户",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties"
    },
    {
      "name": "minio.bucket-name",
      "type": "java.lang.String",
      "description": "默认桶名称, 默认为空, 表示当前应用未指定默认使用的桶, 则在调用MinIO服务时需要显示指定桶名称",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties"
    },
    {
      "name": "minio.connect-timeout",
      "type": "java.lang.Long",
      "description": "设置连接超时时间, 单位毫秒, 默认0, 表示没有超时限制",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": 0
    },
    {
      "name": "minio.create-bucket-if-not-exists",
      "type": "java.lang.Boolean",
      "description": "桶不存在时是否创建, 默认是, 该配置项仅在 bucketName 不为空时有效",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": true
    },
    {
      "name": "minio.enable",
      "type": "java.lang.Boolean",
      "description": "是否启用 MinIO, 默认 true",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": true
    },
    {
      "name": "minio.endpoint",
      "type": "java.lang.String",
      "description": "MinIO IP地址",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties"
    },
    {
      "name": "minio.port",
      "type": "java.lang.Integer",
      "description": "MinIO 端口号, 默认是 9000 (注意: 这里的端口是 API 服务的端口, 不是 Console 服务的端口)",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": 9000
    },
    {
      "name": "minio.read-timeout",
      "type": "java.lang.Long",
      "description": "设置读取超时时间, 单位毫秒, 默认0, 表示没有超时限制",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": 0
    },
    {
      "name": "minio.region",
      "type": "java.lang.String",
      "description": "桶的存储区域",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties"
    },
    {
      "name": "minio.secret-key",
      "type": "java.lang.String",
      "description": "密码",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties"
    },
    {
      "name": "minio.secure",
      "type": "java.lang.Boolean",
      "description": "是否用 HTTPS 访问, 默认否",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": false
    },
    {
      "name": "minio.write-timeout",
      "type": "java.lang.Long",
      "description": "设置写入超时时间, 单位毫秒, 默认0, 表示没有超时限制",
      "sourceType": "com.neweasesoft.dataplus.framework.minio.autoconfigure.MinioProperties",
      "defaultValue": 0
    }
  ],
  "hints": []
}